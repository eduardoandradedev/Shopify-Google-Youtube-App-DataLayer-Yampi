function () {
  if ({{User Data | NORM - City (META)}}) {
    
    // Se tiver informação no DataLayer executa, senão proximo
    return {{User Data | NORM - City (META)}};
  
  } else if ({{Set Cookies | City (META)}}) {
    
    // Se tiver informação nos Cookies executa, senão vazio
    return {{Set Cookies | City (META)}};

  } else {
    return undefined;
  }
}
