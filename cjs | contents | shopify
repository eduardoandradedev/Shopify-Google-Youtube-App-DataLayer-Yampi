function() {
  return {{dlv | ecommerce.items}}.map(function(product) {
    var quantity = parseInt(product.quantity, 10) || 0; 
    var price = parseFloat(product.price) || 0; 
    var itemPrice = price * quantity;

    return {
      id: product.item_id,
      quantity: quantity,
      item_price: itemPrice
    };
  });
}
