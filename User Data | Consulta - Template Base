function () {
  if ({{dlv-variavel}}) {
    
    // Se tiver informação no DataLayer executa, senão proximo
    return {{dlv-variavel}};
  
  } else if ({{input-variavel}}) {
    
    // Se tiver informação no Input do site executa, senão proximo
    return {{input-variavel}};

  } else if ({{cookie-variavel}}) {
    
    // Se tiver informação nos Cookies executa, senão vazio
    return {{cookie-variavel}};

  } else {
    return undefined;
  }
}


// Fazer uma variável de consulta para cada campo capturado via DLV, Input e Cookie
// Vá alterando os campos de acordo com as variáveis que vc já criou anteriormente
